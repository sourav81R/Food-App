# Server
PORT=5000
CLIENT_URL=http://localhost:5173

# Database
MONGODB_URL=mongodb+srv://<db_user>:<db_password>@cluster0.xxxxx.mongodb.net/foodapp?retryWrites=true&w=majority

# Auth
JWT_SECRET=replace_with_secure_secret
JWT_EXPIRE=7d

# Email (OTP)
EMAIL=your_email@gmail.com
EMAIL_PASS=your_app_password

# Cloudinary
CLOUDINARY_CLOUD_NAME=your_cloud_name
CLOUDINARY_API_KEY=your_cloudinary_api_key
CLOUDINARY_API_SECRET=your_cloudinary_api_secret

# Razorpay
# Use matching mode pair:
# - Both must be TEST keys OR both must be LIVE keys
# - Do NOT use sample keys like rzp_test_1DP5mmOlF5G5ag / qwertyui
RAZORPAY_KEY_ID=rzp_test_xxxxxxxxxxxxxx
RAZORPAY_KEY_SECRET=your_razorpay_secret

# Google OAuth (optional)
clientID=your_google_client_id
clientSecret=your_google_client_secret

# Admin bootstrap (optional)
ADMIN_EMAIL=admin@example.com
ADMIN_PASS=strong_admin_password
